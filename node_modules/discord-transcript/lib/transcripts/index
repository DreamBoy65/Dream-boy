.await fetch(encodeURI(endpoint), {
			method: 'post',
			body: JSON.stringify(body),
			headers: { 'Content-Type': 'application/json' },
		})
			.then(res => res.json())
			.then(res => {
				if (res.status !== 200) {
					log.warn(res);
					return new Error(`${res.status} (${res.message})`);
                } else {
                    id = res.message;
                }
                


				

				
				fs.unlinkSync(path.join(__dirname, json));

				
			}).catch(e => {
				return console.log(e)
				
            });